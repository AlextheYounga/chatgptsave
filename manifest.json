{
  "manifest_version": 3,
  "name": "ChatGPT Save",
  "description": "Save conversations from ChatGPT web UI.",
  "version": "0.1.0",
  "action": {
    "default_title": "ChatGPT Save"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://chatgpt.com/*"
  ]
}
